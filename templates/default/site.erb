#
# Generated by Chef for <%= node['fqdn'] %>
# Do NOT modify this file, local modifications will be overwritten.
#
<% unless nil_or_empty?(@options) %>

# Options
<% @options.sort.each do |option, value| %>
<%= option %> <%= template_render_option_values(value) %>
<% end -%>
<% end -%>
<% unless nil_or_empty?(@location) %>

# Location
<% @location.sort.each do |loc, options| %>
<%= render('partials/location.erb', cookbook: 'apache_httpd', variables: { location: loc, options: options }) %>

<% end -%>
<% end -%>
<% unless nil_or_empty?(@directory) %>

# Directory
<% @directory.sort.each do |dir, options| %>
<%= render('partials/directory.erb', cookbook: 'apache_httpd', variables: { directory: dir, options: options }) %>

<% end -%>
<% end -%>
<% unless nil_or_empty?(@virtualhost) %>

# Virtual Host
<% @virtualhost.each do |listen, options| %>
<%= render('partials/virtualhost.erb', cookbook: 'apache_httpd', variables: { listen: listen, options: options.fetch('options', nil), location: options.fetch('location', nil), directory: options.fetch('directory', nil) }) %>

<% end -%>
<% end -%>
