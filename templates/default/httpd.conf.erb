#
# Generated by Chef for <%= node['fqdn'] %>
# Do NOT modify this file, local modifications will be overwritten.
#

<% unless nil_or_empty?(@options) %>
# Options
<% @options.sort.each do |option, value| %>
<%= template_partial_indent(render('partials/_option.erb', cookbook: 'apache_httpd', variables: { option: option, value: value }), 1, 4) %>
<% end %>
<% end %>
<% unless nil_or_empty?(@include) %>

# Include
<% @include.sort.each do |include_file| %>
Include <%= include_file %>
<% end %>
<% end %>
<% unless nil_or_empty?(@include_optional) %>

# Include Optional
<% @include_optional.sort.each do |include_file| %>
IncludeOptional <%= include_file %>
<% end %>
<% end %>
<% unless nil_or_empty?(@directory) %>

# Dictories
<% @directory.sort.each do |dir, options| %>
<%= render('partials/directory.erb', cookbook: 'apache_httpd', variables: { directory: dir, options: options }) %>
<% end %>
<% end %>
<% unless nil_or_empty?(@ifmodule) %>

# If Modules
<% @ifmodule.sort.each do |mod, options| %>
<%= render('partials/ifmodule.erb', cookbook: 'apache_httpd', variables: { module: mod, options: options }) %>
<% end %>
<% end %>
<% unless nil_or_empty?(@files) %>

# Files
<% @files.sort.each do |file, options| %>
<%= render('partials/files.erb', cookbook: 'apache_httpd', variables: { files: file, options: options }) %>
<% end %>
<% end %>
